<div class="container">
  <div class="show_header">
  <h1>Create Survey</h1>
</div>

  <form class="survey">
    <input type="hidden">
    <label>Title: <input type="text" name="title" id="title" required="true"></label><br><br>
    <label>Question: <input type="text" name="question" class="question" required="true"></label>
    <input type="button" value="Next Question" class="submit_question"><br><br><br>
      <input type="button" value="Add Choice" class="add_choice"><br>
    <ul >
      <div class="more_choice">
      <li><label>Choice: <input type="text" name="choices[]" class="choice" required="true"></label></li>
    </div>
    </ul>
    <br><br>
    <input type="submit" value="Generate Survey">
  </form>


  <div id="survey_display">
  </div>

  <div class="url_display">
  </div>
</div>